{
    "[python]": {
        "editor.defaultFormatter": "charliermarsh.ruff",
        "editor.formatOnSave": true
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
    },
    "cSpell.words": [
        "celerybeat",
        "Connor",
        "cookiecutter",
        "cython",
        "dmypy",
        "htmlcov",
        "ipynb",
        "ipython",
        "leetcode",
        "mkdocs",
        "mypy",
        "nooby",
        "norecursedirs",
        "nosetests",
        "nums",
        "peekable",
        "pipenv",
        "Pipfile",
        "pybuilder",
        "pycache",
        "pycodestyle",
        "pyenv",
        "pyflow",
        "pypa",
        "pypackages",
        "pyrightconfig",
        "pytest",
        "pytype",
        "ropeproject",
        "rstest",
        "rtype",
        "scrapy",
        "sdist",
        "shellcheck",
        "snakecase",
        "spacecadet",
        "Spyder",
        "spyderproject",
        "spyproject",
        "summands",
        "testpaths",
        "vandervoort",
        "webassets",
        "wumbo"
    ],
    "cSpell.ignorePaths": [
        "package-lock.json",
        "node_modules",
        "vscode-extension",
        ".git/{info,lfs,logs,refs,objects}/**",
        ".git/{index,*refs,*HEAD}",
        ".vscode",
        ".vscode-insiders",
        "**/test_*",
    ],
    "python.testing.pytestArgs": [
        "."
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true
}
